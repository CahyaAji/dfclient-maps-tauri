{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "core:webview:default",
      "permissions": [
        "allow-open-urls",
        "allow-navigation",
        "allow-window",
        "allow-clipboard-read-text",
        "allow-clipboard-write-text",
        "allow-file-drop",
        "allow-devtools",
        "allow-geolocation"
      ]
    }
  ]
}
